   "source": [
    "import re\n",
    "\n",
    "text = 'He was carefully disguised but captured quickly by police.'\n",
    "\n",
    "compile_p = re.compile(r'\\S\\w*ly')\n",
    "\n",
    "output1 = compile_p.finditer(text)\n",
    "\n",
    "for m in output1:\n",
    "    s = m.start()\n",
    "    e = m.end()\n",
    "    adv = m.group()\n",
    "    print('%d-%d:%s' % (s,e,adv))"
   ]
